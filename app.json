{
  "expo": {
    "name": "site-manager-app",
    "slug": "site-manager-app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "packagerOpts": {
      "sourceExts": [
        "js",
        "json",
        "ts",
        "tsx",
        "jsx",
        "vue"
      ]
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1024.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bam.site-manager-app",
      "buildNumber": "1.0.0"
    }
  }
}